<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
        default-autowire="constructor"
        >

    <bean class="octopus.teamcity.ui.OctopusReleasePathsImpl"/>
    <bean class="octopus.teamcity.ui.OctopusReleaseFeature"/>
    <bean class="octopus.teamcity.ui.OctopusReleaseController"/>

    <bean class="octopus.teamcity.octo.OctoRunner"/>

    <bean class="octopus.teamcity.BuildStatusListener"/>
</beans>
